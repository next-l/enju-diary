<h1>Listing manifestations</h1>

<table>
  <tr>
    <th>Original title</th>
    <th>Isbn</th>
    <th>Date of publication</th>
    <th>Url</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @manifestations.each do |manifestation| %>
  <tr>
    <td><%= manifestation.original_title %></td>
    <td><%= manifestation.isbn %></td>
    <td><%= manifestation.date_of_publication %></td>
    <td><%= manifestation.url %></td>
    <td><%= link_to 'Show', manifestation %></td>
    <td><%= link_to 'Edit', edit_manifestation_path(manifestation) %></td>
    <td><%= link_to 'Destroy', manifestation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Manifestation', new_manifestation_path %>
