<h1>Listing realizes</h1>

<table>
  <tr>
    <th>Patron</th>
    <th>Manifestation</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @realizes.each do |realize| %>
  <tr>
    <td><%= realize.patron_id %></td>
    <td><%= realize.manifestation_id %></td>
    <td><%= link_to 'Show', realize %></td>
    <td><%= link_to 'Edit', edit_realize_path(realize) %></td>
    <td><%= link_to 'Destroy', realize, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

